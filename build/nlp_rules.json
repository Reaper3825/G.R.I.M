[
    {
      "intent": "open_app",
      "description": "Open a local application by name",
      "pattern": "^\\s*(\\w+)\\s+([\\w\\.\\-]+)\\s*$",
      "slot_names": ["verb", "app"],
      "score_boost": 0.3,
      "case_insensitive": true
}

  {
    "intent": "search_web",
    "description": "Search the web for a query",
    "pattern": "^(google|search|look up)\\s+(.+)$",
    "slot_names": ["verb","query"],
    "score_boost": 0.2,
    "case_insensitive": true
  },
  {
    "intent": "set_timer",
    "description": "Set a timer in minutes",
    "pattern": "^(set\\s+)?timer\\s+for\\s+(\\d+)\\s*(minutes|min|m)\\b",
    "slot_names": ["_opt","minutes"],
    "score_boost": 0.25,
    "case_insensitive": true
  },
  {
    "intent": "clean",
    "description": "Remove files from current directory",
    "pattern": "^\\s*clean\\b.*$",
    "slot_names": [],
    "score_boost": 0.25,
    "case_insensitive": true
  }
]
